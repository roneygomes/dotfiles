{
  "[dockercompose]": {
    "editor.autoIndent": "advanced",
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[terraform-vars]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "breadcrumbs.enabled": true,
  "catppuccin-icons.specificFolders": false,
  "cursor.chat.terminalShowHoverHint": false,
  "cursor.composer.queueMessageDefaultBehavior": "stop-and-send",
  "cursor.composer.shouldAllowCustomModes": true,
  "cursor.composer.usageSummaryDisplay": "always",
  "debug.console.closeOnEnd": true,
  "debug.console.fontFamily": "Iosevka Fixed",
  "diffEditor.codeLens": true,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": false,
  "editor.accessibilitySupport": "off",
  "editor.autoSurround": "quotes",
  "editor.bracketPairColorization.enabled": false,
  "editor.codeActions.triggerOnFocusChange": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.codeLensFontFamily": "Sans-Serif",
  "editor.codeLensFontSize": 11,
  "editor.cursorBlinking": "solid",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Iosevka Fixed",
  "editor.fontSize": 13.5,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "editor.guides.indentation": false,
  "editor.lineHeight": 1.5,
  "editor.matchBrackets": "near",
  "editor.minimap.enabled": false,
  "editor.renderLineHighlight": "none",
  "editor.renderWhitespace": "none",
  "editor.rulers": [
    120
  ],
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.wordWrap": "on",
  "eslint.format.enable": true,
  "eslint.ignoreUntitled": true,
  "eslint.nodeEnv": null,
  "eslint.quiet": true,
  "eslint.run": "onSave",
  "eslint.runtime": "node",
  "eslint.useESLintClass": true,
  "eslint.validate": [
    "typescript"
  ],
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.decorations.colors": false,
  "explorer.excludeGitIgnore": false,
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "afterDelay",
  "files.exclude": {
    ".cursor": false,
    ".husky": true,
    ".idea": true,
    ".junie": true,
    ".nx": true,
    ".vscode": true,
    ".yarn": true,
    "dist": true
  },
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "never",
  "git.useEditorAsCommitInput": true,
  "git.verboseCommit": true,
  "githubPullRequests.createDefaultBaseBranch": "repositoryDefault",
  "githubPullRequests.defaultMergeMethod": "rebase",
  "githubPullRequests.fileListLayout": "flat",
  "gitlens.currentLine.fontFamily": "Sans-Serif",
  "gitlens.currentLine.fontSize": 12,
  "gitlens.launchpad.indicator.enabled": false,
  "gotoNextPreviousMember.symbolKinds": [
    "method",
    "function",
    "enum",
    "interface",
    "constructor"
  ],
  "jestrunner.changeDirectoryToWorkspaceRoot": true,
  "jestrunner.jestCommand": "run-test.js ${relativeFile}",
  "jestrunner.runOptions": [],
  "json.schemaDownload.enable": true,
  "markdown-mermaid.lightModeTheme": "base",
  "projectManager.git.baseFolders": [
    "~/projects"
  ],
  "projectManager.git.ignoredFolders": [
    "node_modules",
    "out",
    "typings",
    "test",
    ".haxelib",
    "target",
    "dist"
  ],
  "projectManager.groupList": true,
  "projectManager.openInNewWindowWhenClickingInStatusBar": true,
  "projectManager.projectsLocation": "~/projects",
  "projectManager.removeCurrentProjectFromList": false,
  "projectManager.sortList": "Recent",
  "python.languageServer": "None",
  "redhat.telemetry.enabled": false,
  "security.promptForLocalFileProtocolHandling": false,
  "security.workspace.trust.untrustedFiles": "open",
  "shellcheck.ignorePatterns": {
    "**/*.env": true,
    "**/*.env.*": true
  },
  "task.problemMatchers.neverPrompt": {
    "nx": true
  },
  "terminal.integrated.fontFamily": "Iosevka Term",
  "terminal.integrated.fontSize": 13.5,
  "typescript.implementationsCodeLens.enabled": false,
  "typescript.referencesCodeLens.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "vim.foldfix": true,
  "vim.smartRelativeLine": false,
  "vim.useSystemClipboard": true,
  "window.autoDetectHighContrast": false,
  "window.newWindowDimensions": "maximized",
  "window.restoreFullscreen": true,
  "window.restoreWindows": "none",
  "window.zoomLevel": 1.1,
  "window.zoomPerWindow": false,
  "workbench.colorCustomizations": {
    "[Nord Light Brighter]": {
      "editor.background": "#fbf7f0",
      "editor.selectionHighlightBorder": "default",
      "editor.wordHighlightBackground": "#eceeeee6",
      "editor.wordHighlightBorder": "default",
      "editor.wordHighlightStrongBackground": "#eceeeee6",
      "editor.wordHighlightStrongBorder": "default",
      "editor.wordHighlightTextBorder": "default",
      "editorCodeLens.foreground": "#929292",
      "editorGroupHeader.border": "#d3d3d3",
      "gitlens.trailingLineForegroundColor": "#929292",
      "panelTitle.border": "#d3d3d3",
      "sideBar.foreground": "#000000",
      "terminal.background": "#fbf7f0",
      "terminal.foreground": "#000000"
    }
  },
  "workbench.colorTheme": "Nord Light Brighter",
  "workbench.editor.enablePreview": false,
  "workbench.editor.showTabs": "single",
  "workbench.hover.delay": 3000,
  "workbench.iconTheme": "catppuccin-latte",
  "workbench.settings.applyToAllProfiles": [
    "editor.fontFamily",
    "window.zoomLevel"
  ],
  "workbench.settings.editor": "json",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "workbench.tree.renderIndentGuides": "none"
}